{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest Current File",
      "program": "${workspaceFolder}/scripts/jest/jest-cli.js",
      "args": ["${fileBasenameNoExtension}", "--source-maps=true"],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "env": {
        "NODE_ENV": "production"
      }
    }
  ]
}